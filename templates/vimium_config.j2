{
  "ansibleFile": "Chromium Vimium extension configuration.",
  "ansibleConfig": "Ansible cannot add this config automatically.",
  "ansibleInstallSteps": "Go to Chromium extensions. Go to Vimium options. Load this json file via the Restore button. Click Save Changes button.",
  "settingsVersion": "1.66",
  "keyMappings": "\nmap j scrollDown \nmap k scrollUp \nmap H scrollLeft \nmap L scrollRight \nmap , previousTab \nmap . nextTab \nmap h goBack \nmap l goForward",
  "hideHud": false,
  "linkHintCharacters": "sadfjklewcmpgh",
  "linkHintNumbers": "0123456789",
  "newTabUrl": "about:newtab",
  "scrollStepSize": 60,
  "smoothScroll": true,
  "grabBackFocus": false,
  "searchEngines": "w: https://www.wikipedia.org/w/index.php?title=Special:Search&search=%s Wikipedia\n",
  "searchUrl": "https://www.google.com/search?q=",
  "exclusionRules": [
    {
      "pattern": "https?://mail.google.com/*",
      "passKeys": ""
    }
  ],
  "nextPatterns": "next,more,newer,>,›,→,»,≫,>>",
  "previousPatterns": "prev,previous,back,older,<,‹,←,«,≪,<<",
  "filterLinkHints": false,
  "waitForEnterForFilteredHints": true,
  "regexFindMode": false,
  "ignoreKeyboardLayout": false,
  "userDefinedLinkHintCss": "div > .vimiumHintMarker {\n/* linkhint boxes */\nbackground: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#FFF785),\n  color-stop(100%,#FFC542));\nborder: 1px solid #E3BE23;\n}\n\ndiv > .vimiumHintMarker span {\n/* linkhint text */\ncolor: black;\nfont-weight: bold;\nfont-size: 12px;\n}\n\ndiv > .vimiumHintMarker > .matchingCharacter {\n}"
}

